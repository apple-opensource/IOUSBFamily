# Jamfile for target "UPS Test Tool" of project "IOUSBFamily"


JAVAC_SOURCE_FILE_ENCODING default = MacRoman ;

# Implicitly computed project-wide settings for project
export deferred DEVELOPMENT_LANGUAGE = English ;
export deferred PROJECT = $(PROJECT_NAME) ;
export deferred PROJECT_NAME = IOUSBFamily ;
export deferred SRCROOT = /Volumes/Home/Latest/PantherUpdates/Bisque/IOUSBFamily ;

# User-defined project-wide settings for project

# Target "UPS Test Tool"

# Implicitly computed build settings for target "UPS Test Tool"
export deferred CLONE_HEADERS = NO ;
export deferred PFE_FILE_C_DIALECTS = c ;
export deferred TARGETNAME = $(TARGET_NAME) ;
export deferred TARGET_NAME = "UPS Test Tool" ;

# User-defined build settings for target "UPS Test Tool"
export deferred INSTALL_PATH = "" ;
export deferred OTHER_CFLAGS = -DSTAND_ALONE_TEST_TOOL=1  ;
export deferred OTHER_LDFLAGS =  ;
export deferred OTHER_REZFLAGS =  ;
export deferred PRODUCT_NAME = UPSTestTool ;
export deferred REZ_EXECUTABLE = YES ;
export deferred SECTORDER_FLAGS =  ;
export deferred WARNING_CFLAGS = -W -Wall -Wno-unused -Wno-four-char-constants -Wno-unknown-pragmas ;

if "$(BUILD_STYLE)" = Development {
	export deferred COPY_PHASE_STRIP = NO ;
	export deferred OPTIMIZATION_CFLAGS = -O0 ;
}

if "$(BUILD_STYLE)" = Deployment {
	export deferred COPY_PHASE_STRIP = YES ;
	export deferred OPTIMIZATION_CFLAGS = -O3 ;
}


ConditionalDefinitions _ ;
commitdeferred ;


BeginTool "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" ;

# Headers
HEADERS = ;
ProcessHeaders "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" : "$(HEADERS)" : "" : BPTag000 ;

# Sources
SOURCES = Configd/upsconfigd.c ;
ProcessSources "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" : "$(SOURCES)" : BPTag000 : BPTag001 ;

# Frameworks & Libraries
/System/Library/Frameworks/CoreFoundation.framework_ATTRIBUTES = AbsolutePath ;
/System/Library/Frameworks/IOKit.framework_ATTRIBUTES = AbsolutePath ;
/System/Library/Frameworks/SystemConfiguration.framework_ATTRIBUTES = AbsolutePath ;
FRAMEWORKS_AND_LIBRARIES =
	/System/Library/Frameworks/CoreFoundation.framework
	/System/Library/Frameworks/IOKit.framework
	/System/Library/Frameworks/SystemConfiguration.framework ;
ProcessFrameworksAndLibraries "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" : "$(FRAMEWORKS_AND_LIBRARIES)" : BPTag001 : BPTag002 ;

# Rez Files
REZ_FILES = ;
ProcessRezFiles "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" : "$(REZ_FILES)" : BPTag002 : BPTag003 ;

rule DefineHeaderDependencies
{
}

DefineHeaderDependencies "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" ;

EndTool "<UPSTestTool>UPSTestTool$(TOOL_SUFFIX)" ;

